4C\\u0448\\u0435 \\u0432\\u043E\\u0437\\u043C\\u043E\\u0436\\u043D\\u043E\\u0441\\u0442\\u0435\\u0439 \\u0434\\u043E\\u0441\\u0442\\u0438\\u0436\\u0435\\u043D\\u0438\\u044F \\u0443\\u0441\\u043F\\u0435\\u0445\\u0430 \\u0432 \\u0443\\u0441\\u043B\\u043E\\u0432\\u0438\\u044F\\u0445 \\u043A\\u043E\\u043D\\u043A\\u0443\\u0440\\u0435\\u043D\\u0446\\u0438\\u0438 \\u0437\\u0430 \\u043E\\u0433\\u0440\\u0430\\u043D\\u0438\\u0447\\u0435\\u043D\\u043D\\u044B\\u0435 \\u0440\\u0435\\u0441\\u0443\\u0440\\u0441\\u044B. \\u041A\\u043E\\u043B\\u043B\\u0430\\u0431\\u043E\\u0440\\u0430\\u0446\\u0438\\u044F \\u043C\\u043E\\u0436\\u0435\\u0442 \\u0441\\u0443\\u0449\\u0435\\u0441\\u0442\\u0432\\u043E\\u0432\\u0430\\u0442\\u044C \\u0438 \\u043F\\u0440\\u0438 \\u043F\\u0440\\u043E\\u0442\\u0438\\u0432\\u043E\\u043F\\u043E\\u043B\\u043E\\u0436\\u043D\\u043E\\u0441\\u0442\\u0438 \\u0446\\u0435\\u043B\\u0435\\u0439, \\u043D\\u043E \\u0432 \\u044D\\u0442\\u043E\\u043C \\u043A\\u043E\\u043D\\u0442\\u0435\\u043A\\u0441\\u0442\\u0435 \\u0434\\u0430\\u043D\\u043D\\u043E\\u0435 \\u043F\\u043E\\u043D\\u044F\\u0442\\u0438\\u0435 \\u0438\\u0441\\u043F\\u043E\\u043B\\u044C\\u0437\\u0443\\u0435\\u0442\\u0441\\u044F \\u0440\\u0435\\u0434\\u043A\\u043E.\"));\n}\n\nexport default AboutPlatform;","map":{"version":3,"sources":["C:/Users/a-pol/react-website-v1/src/components/MainPage/Elements/AboutPlatform.js"],"names":["React","AboutPlatform"],"mappings":";AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAO,6BAAP;;AAEA,SAASC,aAAT,GAAwB;AACpB,sBACI;AAAK,IAAA,SAAS,EAAC,MAAf;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kBACI;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,qEADJ,eAEI;AAAG,IAAA,SAAS,EAAC,MAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,krCAFJ,eAMI;AAAG,IAAA,SAAS,EAAC,MAAb;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,m0EANJ,CADJ;AAgBH;;AAED,eAAeA,aAAf","sourcesContent":["import React from \"react\";\r\nimport '../Styles/AboutPlatform.css'\r\n\r\nfunction AboutPlatform(){\r\n    return(\r\n        <div className='info'>\r\n            <h1>О Платформе</h1>\r\n            <p className='text'>Коллаборация (сотрудничество) — процесс совместной деятельности\r\n            в какой-либо сфере двух и более людей или организаций для достижения общих целей, при которой\r\n            происходит обмен знаниями, обучение и достижение согласия (консенсуса).\r\n            </p>\r\n            <p className='text'>Как правило, этот процесс требует наличия руководящего органа, при этом форма руководства\r\n            может быть и общественной при сотрудничестве равноправных членов децентрализованного \r\n            сообщества. Считается, что участники коллаборации могут получить больше возможностей\r\n            достижения успеха в условиях конкуренции за ограниченные ресурсы. Коллаборация может\r\n            существовать и при противоположности целей, но в этом контексте данное понятие \r\n            используется редко.\r\n            </p>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default AboutPlatform;"]},"metadata":{},"sourceType":"module"}